# Application Configuration
NODE_ENV=production
PORT=3000

# GitHub Configuration (Required)
GITHUB_TOKEN=your_github_token
NEXT_PUBLIC_GITHUB_OWNER=your_github_username_or_organization

# AI Model Configuration (Required - At least one model must be configured)
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

DATABASE_URL=postgres_with_pgvector_extension_url

EMBEDDING_PROVIDER=titan

# app settings
AI_SERVICE_USE_CACHE=true

# Docker-specific Configuration
DOCKER_HEALTHCHECK_ENDPOINT=/api/health